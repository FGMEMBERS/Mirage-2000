<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <default>
    <menu n="10">
      <label>Mirage 2000</label>
      <enabled type="bool">true</enabled>
      <item n="0">
        <label>Select Livery</label>
        <binding>
          <command>nasal</command>
          <script>aircraft.livery.dialog.toggle()</script>
        </binding>
      </item>
      <item n="1">
        <label>MP Shot Report</label>
        <binding>
          <command>nasal</command>
          <script>missile.MPReport()</script>
        </binding>
      </item>
      <item n="2">
        <label>Reload Gun</label>
        <binding>
          <command>nasal</command>
          <script>guns.reload_Cannon()</script>
        </binding>
      </item>
      <item n="3">
        <label>No load</label>
        <binding>
          <command>nasal</command>
          <script>load.NoLoad()</script>
        </binding>
      </item>
      <item n="4">
        <label>Air to Air Middle Distance</label>
        <binding>
          <command>nasal</command>
          <script>load.AirToAirMiddle()</script>
        </binding>
      </item>
      <item n="5">
        <label>Air to Air Short Distance</label>
        <binding>
          <command>nasal</command>
          <script>load.AirToAirshort()</script>
        </binding>
      </item>
      <item n="6">
        <label>Air to Air Long Distance</label>
        <binding>
          <command>nasal</command>
          <script>load.AirToAirLong()</script>
        </binding>
      </item>
      <item n="7">
        <label>Air to Ground</label>
        <binding>
          <command>nasal</command>
          <script>load.AirToGround()</script>
        </binding>
      </item> 
      <item n="8">
        <label>Target Tracking</label>
        <name>track_target</name>
        <binding>
           <command>dialog-show</command>
           <dialog-name>track_target</dialog-name>
        </binding>
      </item> 
    </menu>
  </default>

</PropertyList>
