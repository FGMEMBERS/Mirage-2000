  <key n="68">
    <name>D</name>
    <desc>Passenger door</desc>
    <binding>
      <command>nasal</command>
      <script>doors.passenger.toggle();</script>
    </binding>
  </key>
  
  <key n="100">
    <name>d</name>
    <desc>Doors</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crew.toggle();</script>
    </binding>
  </key>
  
  <key n="69">
    <name>E</name>
    <desc>Parachute</desc>
    <binding>
      <command>nasal</command>
