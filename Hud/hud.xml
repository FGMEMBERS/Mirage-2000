<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <enable3d>true</enable3d>
    
  <label>
    <name>G Load</name>
    <x> -43 </x>
    <y>   2 </y>
    <width>1.5</width>
    <height>1.5</height>
    <input>
      <property>/accelerations/pilot/z-accel-fps_sec</property>
      <factor>-0.03108095</factor>
      <damp>1.0</damp>
    </input>
    <format>%2.1f</format>
    <halign>left</halign>
  </label>

  <ladder>
    <name>Pitch Ladder</name>
    <type>pitch</type>
    <x> -12 </x>
    <y> -18 </y>
    <width>25</width>
    <height>9</height>
    <roll-input>
      <property>/orientation/roll-deg</property>
    </roll-input>
    <pitch-input>
      <property>/orientation/pitch-deg</property>
    </pitch-input>
    <compression-factor>7.33</compression-factor>
    <display-span>6.0</display-span>
    <divisions>2.0</divisions>
    <screen-hole>20</screen-hole>
    <zero-bar-overlength>15</zero-bar-overlength>
    <enable-dive-bar-angle>false</enable-dive-bar-angle>
    <enable-dynamic-origin>false</enable-dynamic-origin>
    <enable-fuselage-ref-line>true</enable-fuselage-ref-line>
    <enable-target-spot>true</enable-target-spot>
    <enable-target-markers>true</enable-target-markers>
    <enable-target-markers>true</enable-target-markers>
    <enable-velocity-vector>false</enable-velocity-vector>
    <enable-drift-marker>false</enable-drift-marker>
    <enable-alpha-bracket>false</enable-alpha-bracket>
    <enable-energy-marker>false</enable-energy-marker>
    <enable-climb-dive-marker>false</enable-climb-dive-marker>
    <enable-glide-slope-marker>false</enable-glide-slope-marker>
    <glide-slope>0.0</glide-slope>
    <enable-energy-worm>false</enable-energy-worm>
    <enable-waypoint-marker>false</enable-waypoint-marker>
  </ladder>

  <label>
    <name>Gyro Heading Label</name>
    <x> -9 </x>
    <y>  5 </y>
    <width>18</width>
    <height>15</height>
    <input>
      <property>/orientation/heading-deg</property>
    </input>
    <format>%4.0f</format>
    <halign>center</halign>
    <box>true</box>
  </label>

  <label>
    <name>KIAS</name>
    <x> -40 </x>
    <y> -55 </y>
    <width>24</width>
    <height>12</height>
    <input>
      <property>/velocities/airspeed-kt</property>
    </input>
    <format>%4.0f</format>
    <halign>left</halign>
    <box>true</box>
    <option>right</option>
  </label>

  <label>
    <name>Altitude</name>
    <x>  12 </x>
    <y> -55 </y>
    <width>30</width>
    <height>12</height>
    <input>
      <property>/position/altitude-ft</property>
    </input>
    <format>%4.0f</format>
    <halign>right</halign>
    <box>true</box>
    <option>left</option>
  </label>

</PropertyList>

