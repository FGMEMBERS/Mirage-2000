<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <sim include="Nasal/views.xml">

    <description>Mirage 2000 (YASim)</description>
    <author>BARANGER Emmanuel (3D/FDM), Buckaroo (FDM), 5H1N0B1 (Weapons, system), F-Sig (3D, ssystems)</author>
    <aircraft-version>0.1</aircraft-version>
    <status>0.1 Alpha</status>

    <fuel-fraction>0.8</fuel-fraction>

    <instrumentation>
         <path>Aircraft/Mirage-2000/System/instrumentation.xml</path>
    </instrumentation>

    <model>
      <path>Aircraft/Mirage-2000/Models/m2000-5.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <submodels> 
      <serviceable type="bool">true</serviceable>        
      <path>Aircraft/Mirage-2000/System/submodels.xml</path>
    </submodels>

    <startup>
      <splash-texture>Aircraft/Mirage-2000/m2000-5-splash.png</splash-texture>
    </startup>

    <sound>
      <path>Aircraft/Mirage-2000/Sounds/m2000-5-sound.xml</path>
    </sound>

    <rendering>
      <redout>
        <parameters>
          <blackout-complete-g>9</blackout-complete-g>
          <blackout-onset-g>6</blackout-onset-g>
        </parameters>
      </redout>
    </rendering>
        
    <multiplay>
      <chat_display>1</chat_display>
            <generic> 
        <int type="int" n="0">  0 </int>                                           <!--  0 immat -->
        <int type="int" n="1">  0 </int>                                           <!--  1 immat -->
        <int type="int" n="2">  0 </int>                                           <!--  2 immat -->
        <int type="int" n="3">  0 </int>                                           <!--  3 immat -->
        <int type="int" n="4">  0 </int>                                           <!--  4 immat -->
        <int type="int" n="5">  0 </int>                                           <!--  5 immat 
        <int n="10" >0</int>                                          
        <int n="11" >0</int>                                                  
        <int n="12" >0</int>                                             
        <int n="13" >0</int>                                          
        <int n="14" >0</int>                                            
        <int n="15" >0</int>                                          
        <int n="16" >0</int>                                             -->
        <int type="bool" n="6" alias="/smoke/active"> 0 </int>                     <!--  6 start smoke -->
        <int type="int" n="7" alias="smoke/particlepersec"> 50 </int>              <!--  7 particlepersec -->
        <int type="int" n="10" alias="controls/armament/trigger">0</int>           <!-- 10 reserved for bombable mode -->
        <float n="0" alias="/instrumentation/doors/crew/position-norm"/>
        <float n="1" alias="/instrumentation/doors/passenger/position-norm"/>
        <float n="2" alias="/position/gear-agl-m"> 0 </float>                      <!-- altitude for MP shadow -->
        <float n="3" alias="/smoke/colors/knob-red">1</float>                      <!-- 3 smoke mixer red setting -->
        <float n="4" alias="/smoke/colors/knob-green">1</float>                    <!-- 4 smoke mixer green setting -->
        <float n="5" alias="/smoke/colors/knob-blue">1</float>                     <!-- 5 smoke mixer blue setting -->
        <float n="8" alias="/engines/engine[0]/afterburner"/>
        <string n="0" type="string" />
        <string n="9" type="string" />                                             <!-- string[9] reserved for bombable mode -->
           <!--                                          For loads 
        <string n="11" alias="/sim/weight[1]/selected"/>                                                      
        <string n="12" alias="/sim/weight[2]/selected"/>                                              
        <string n="13" alias="/sim/weight[3]/selected"/>                                             
        <string n="14" alias="/sim/weight[4]/selected"/>                                             
        <string n="15" alias="/sim/weight[5]/selected"/>                                             
        <string n="16" alias="/sim/weight[6]/selected"/>                                         -->
      </generic>
    </multiplay>

    <hud>
      <path n="1">Aircraft/Mirage-2000/Hud/hud.xml</path>
      <visibility n="1">true</visibility>
      <palette>
        <color n="0">
          <alpha type="float">0.85</alpha>
          <antialiased type="bool">true</antialiased>
          <brightness type="float">0.85</brightness>
          <transparent type="bool">true</transparent>
          <red type="float">0.38</red>
          <green type="float">1.0</green>
          <blue type="float">0.22</blue>
        </color>
        <color n="1">
          <alpha type="float">0.85</alpha>
          <antialiased type="bool">true</antialiased>
          <brightness type="float">0.85</brightness>
          <transparent type="bool">true</transparent>
          <red type="float">   1.0 </red>
          <green type="float"> 0.2 </green>
          <blue type="float">  0.0 </blue>
        </color>
      </palette>
      <!--Thanks f16 team for that -->
      <clipping>
            <left type="double">-65</left>
            <right type="double">65</right>
            <top type="double">60</top>         <!--   20 -->
            <bottom type="double">-88</bottom>  <!-- -102 -->
      </clipping>
    </hud>

    <help>
      <title>Mirage 2000</title>
      <key>
        <name>d</name>
        <desc>Open/Close canopy</desc>
      </key>
      <key>
        <name>e</name>
        <desc>Fire guns/missiles</desc>
      </key>
      <key>
        <name>E</name>
        <desc>Parachute</desc>
      </key>
      <key>
        <name>Pav Num</name>
        <desc>(Release the correspondant Load)</desc>
      </key>
      <key>
        <name>0</name>
        <desc>Release 2,3,4 (Jettison Tank)</desc>
      </key>
      <key>
        <name>T</name>
        <desc>Toggle RWR</desc>
      </key>
      <key>
        <name>t</name>
        <desc>Change Radar Distances (10, 20 50,100,150)</desc>
      </key>
      <key>
        <name>R</name>
        <desc>Activate/desactivate targeting the nearest (the target have to be bellow radar range)</desc>
      </key>
      <key>
        <name>H</name>
        <desc>Toggle Armements Circles</desc>
      </key>
      <key>
        <name>}</name>
        <desc>On/Off Reactor</desc>
      </key>
      <key>
        <name>l</name>
        <desc>Flare out</desc>
      </key>
      <key>
        <name>n</name>
        <desc>Next Target</desc>
      </key>
      <key>
        <name>Ctrl-n</name>
        <desc>Previous Target</desc>
      </key>  
      <key>
        <name>w</name>
        <desc>Cycle Stick Weapon Mode Selector</desc>
      </key>    
      <line>Afterburners engage when</line>
      <line>throttle moves past 75%</line>
      <line>To fire a guided Missile, select A/A missile with "w",</line>
      <line>activate "lock" with R, aim with the HUD and fire with "e"</line>
      <line>Beware : it's a bad idea to fire a missile and have negative G...(Don't shoot yourself)</line>
    </help>
        
