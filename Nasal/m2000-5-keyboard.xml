<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <key n="101">
    <name>e</name>
    <desc>Guns</desc>
    <binding>
      <command>nasal</command>
      <script>guns.fire_MG()</script>
    </binding>
    <mod-up>
      <binding>
        <command>nasal</command>
        <script>guns.stop_MG()</script>
      </binding>
    </mod-up>
  </key>

  <key n="68">
