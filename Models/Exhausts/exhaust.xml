<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>exhaust.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>tuyere01</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>tuyere01</object-name>
    <property>/engines/engine[0]/afterburner</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>   6 </dep></entry>
      <entry><ind> 1 </ind><dep> -10 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

