   </text>
   
   <animation>
     <type>select</type>
     <object-name>gs-inrange</object-name>
     <condition>
       
       <property>autopilot/internal/gs-in-range</property>
       
     </condition>
   </animation>
   
   <!--Altitude-->
   <text>
     <name>alt</name>
     <offsets>
       <x-m>0.0</x-m>
       <y-m>0.060</y-m>
       <z-m>-0.0</z-m>
     </offsets>
     <alignment>center-center</alignment>
     <axis-alignment>yz-plane</axis-alignment>
     
     <type>number-value</type>
     <property>instrumentation/altimeter/indicated-altitude-ft</property>
     <format>%5.0f</format>
     <truncate>true</truncate>    
     
     <font>helvetica_bold.txf</font>
     <character-size>0.009</character-size>
     <character-aspect-ratio type="double">1.3</character-aspect-ratio>
     <font-resolution>
       <width type="int">64</width>
       <height type="int">64</height>
     </font-resolution>
   </text>
   
   <!--Vertical speed plus-->
   <text>
     <name>vs+</name>
     <offsets>
       <x-m>0.0</x-m>
       <y-m>0.059</y-m>
       <z-m>0.008</z-m>
     </offsets>
     <alignment>center-center</alignment>
     <axis-alignment>yz-plane</axis-alignment>
     
     <type>number-value</type>
     <property>autopilot/internal/vert-speed-fpm</property>
     <format>+%5.0f</format>
     <truncate>true</truncate>    
     
     <font>helvetica_bold.txf</font>
     <character-size>0.008</character-size>
     <character-aspect-ratio type="double">1.3</character-aspect-ratio>
     <font-resolution>
       <width type="int">64</width>
       <height type="int">64</height>
     </font-resolution>
   </text>
   
   <animation>
     <type>select</type>
     <object-name>vs+</object-name>
     <condition>
       <greater-than>
         <property>autopilot/internal/vert-speed-fpm</property>
         <value>20</value>
       </greater-than>
     </condition>
   </animation>
   
   <!--Vertical speed moins-->
   <text>
     <name>vs-</name>
     <offsets>
       <x-m>0.0</x-m>
       <y-m>0.059</y-m>
       <z-m>-0.008</z-m>
     </offsets>
     <alignment>center-center</alignment>
     <axis-alignment>yz-plane</axis-alignment>
     
     <type>number-value</type>
     <property>autopilot/internal/vert-speed-fpm</property>
     <format>%5.0f</format>
     <truncate>true</truncate>    
     
     <font>helvetica_bold.txf</font>
     <character-size>0.008</character-size>
     <character-aspect-ratio type="double">1.3</character-aspect-ratio>
     <font-resolution>
       <width type="int">64</width>
       <height type="int">64</height>
     </font-resolution>
   </text>
   
   <animation>
     <type>select</type>
     <object-name>vs-</object-name>
     <condition>
       <less-than>
         <property>autopilot/internal/vert-speed-fpm</property>
         <value>-20</value>
       </less-than>
     </condition>
   </animation>
   
