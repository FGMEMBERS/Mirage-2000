<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <model>
    <name>MyRadar</name>
    <path>mfd.xml</path>
    <offsets>
      <x-m>  0.001</x-m>
      <y-m> -0.001</y-m>
      <z-m> -0.0011 </z-m>
    </offsets>
  </model>
  
  <animation>
    <type>scale</type>
    <object-name>MyRadar</object-name> 
    <x-offset> 0.50 </x-offset>
    <y-offset> 1.10 </y-offset>
    <z-offset> 0.74 </z-offset>
 </animation>

  <model>
    <path>radar.ac</path>
  </model>

  <animation>
    <!-- Objets opaques -->
    <object-name>fond</object-name>
    <object-name>ecran</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>ecran</object-name>    
    <emission>      
      <factor>0.13/</factor>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>
  
   <animation>
      <type>pick</type>
      <object-name>fond</object-name>
      <object-name>ecran</object-name>
      <hovered>
        <binding>
           <command>set-tooltip</command>
           <tooltip-id>radar</tooltip-id>
           <label>Radar</label>
        </binding>
      </hovered>
   </animation>
  
  
  
  
  
  

</PropertyList>
