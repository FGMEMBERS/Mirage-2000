<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>warning.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>texte</object-name>
    <object-name>pa</object-name>
    <object-name>alt</object-name>
    <object-name>tbhm</object-name>
    <object-name>navh</object-name>
    <object-name>lgspeed</object-name>
    <object-name>app</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>texte</object-name>
    <object-name>pa</object-name>
    <object-name>alt</object-name>
    <object-name>tbhm</object-name>
    <object-name>navh</object-name>
    <object-name>lgspeed</object-name>
    <object-name>app</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.6 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>pick</type>
    <object-name>alt</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>  
      <binding>
        <command>property-toggle</command>
        <property>autopilot/locks/altitude</property>
      </binding>
    </action>
  </animation>


</PropertyList>

